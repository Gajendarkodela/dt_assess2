<div class="mt-3 container">

<div class="row">
	<div class="col-lg-1">
		<button onclick="goBack()" class="btn btn-primary">&lt;-Back</button>
  </div>
		<div class="col-lg-10" align="center">
		  <div class="card-box">
		    <h4 class="m-t-0 header-title"> Users List</h4>
		    <p class="text-muted font-14 m-b-20">
		    </p>
		    <table class="table table-bordered " >
		      <thead class="thead-light">
		        <tr>
		         <th>User Id</th>
		         <th>Email</th>
		          <th>Name</th>
		          <th>Action</th>

		        </tr>
		      </thead>
		      <tbody>
		   			<%@users.each do |user|%>
							<tr>
								<td>
									<%=user.id%>
								</td>
								<td>
									<%=user.email%>
								</td>
								<td>
									<%=user.user_name%>
								</td>
								<td>#</td>
							</tr>
					  <%end%>
				  </tbody>
		    </table>
		  </div>
		</div>
  </div>
 </div>
                        